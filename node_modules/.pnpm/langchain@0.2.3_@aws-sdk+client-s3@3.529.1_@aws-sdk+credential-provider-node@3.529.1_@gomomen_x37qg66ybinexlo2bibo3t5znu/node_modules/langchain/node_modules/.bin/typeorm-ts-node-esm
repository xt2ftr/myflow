#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/Flowise/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.3.2_mongodb@6.3.0_gcp-metadata@6.1.0_encoding@0.1.13__socks@2.8.1__m_74l2puklaqlx334ijty3v4mxvy/node_modules/typeorm/node_modules:/home/user/Flowise/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.3.2_mongodb@6.3.0_gcp-metadata@6.1.0_encoding@0.1.13__socks@2.8.1__m_74l2puklaqlx334ijty3v4mxvy/node_modules:/home/user/Flowise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/Flowise/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.3.2_mongodb@6.3.0_gcp-metadata@6.1.0_encoding@0.1.13__socks@2.8.1__m_74l2puklaqlx334ijty3v4mxvy/node_modules/typeorm/node_modules:/home/user/Flowise/node_modules/.pnpm/typeorm@0.3.20_ioredis@5.3.2_mongodb@6.3.0_gcp-metadata@6.1.0_encoding@0.1.13__socks@2.8.1__m_74l2puklaqlx334ijty3v4mxvy/node_modules:/home/user/Flowise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typeorm@0.3.20_ioredis@5.3.2_mongodb@6.3.0_gcp-metadata@6.1.0_encoding@0.1.13__socks@2.8.1__m_74l2puklaqlx334ijty3v4mxvy/node_modules/typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../../../../../typeorm@0.3.20_ioredis@5.3.2_mongodb@6.3.0_gcp-metadata@6.1.0_encoding@0.1.13__socks@2.8.1__m_74l2puklaqlx334ijty3v4mxvy/node_modules/typeorm/cli-ts-node-esm.js" "$@"
fi
