#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/Flowise/node_modules/.pnpm/mammoth@1.7.2/node_modules/mammoth/bin/node_modules:/home/user/Flowise/node_modules/.pnpm/mammoth@1.7.2/node_modules/mammoth/node_modules:/home/user/Flowise/node_modules/.pnpm/mammoth@1.7.2/node_modules:/home/user/Flowise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/Flowise/node_modules/.pnpm/mammoth@1.7.2/node_modules/mammoth/bin/node_modules:/home/user/Flowise/node_modules/.pnpm/mammoth@1.7.2/node_modules/mammoth/node_modules:/home/user/Flowise/node_modules/.pnpm/mammoth@1.7.2/node_modules:/home/user/Flowise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mammoth@1.7.2/node_modules/mammoth/bin/mammoth" "$@"
else
  exec node  "$basedir/../../../../../mammoth@1.7.2/node_modules/mammoth/bin/mammoth" "$@"
fi
