#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../cmake-js@7.3.0/node_modules/cmake-js/bin/cmake-js" "$@"
else
  exec node  "$basedir/../../../../../cmake-js@7.3.0/node_modules/cmake-js/bin/cmake-js" "$@"
fi
