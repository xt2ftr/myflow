#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/Flowise/node_modules/.pnpm/webpack-dev-server@4.15.1_bufferutil@4.0.8_utf-8-validate@6.0.4_webpack@5.90.3_@swc+core@1.4.6_/node_modules/webpack-dev-server/bin/node_modules:/home/user/Flowise/node_modules/.pnpm/webpack-dev-server@4.15.1_bufferutil@4.0.8_utf-8-validate@6.0.4_webpack@5.90.3_@swc+core@1.4.6_/node_modules/webpack-dev-server/node_modules:/home/user/Flowise/node_modules/.pnpm/webpack-dev-server@4.15.1_bufferutil@4.0.8_utf-8-validate@6.0.4_webpack@5.90.3_@swc+core@1.4.6_/node_modules:/home/user/Flowise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/Flowise/node_modules/.pnpm/webpack-dev-server@4.15.1_bufferutil@4.0.8_utf-8-validate@6.0.4_webpack@5.90.3_@swc+core@1.4.6_/node_modules/webpack-dev-server/bin/node_modules:/home/user/Flowise/node_modules/.pnpm/webpack-dev-server@4.15.1_bufferutil@4.0.8_utf-8-validate@6.0.4_webpack@5.90.3_@swc+core@1.4.6_/node_modules/webpack-dev-server/node_modules:/home/user/Flowise/node_modules/.pnpm/webpack-dev-server@4.15.1_bufferutil@4.0.8_utf-8-validate@6.0.4_webpack@5.90.3_@swc+core@1.4.6_/node_modules:/home/user/Flowise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../webpack-dev-server@4.15.1_bufferutil@4.0.8_utf-8-validate@6.0.4_webpack@5.90.3_@swc+core@1.4.6_/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../../../../../webpack-dev-server@4.15.1_bufferutil@4.0.8_utf-8-validate@6.0.4_webpack@5.90.3_@swc+core@1.4.6_/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
