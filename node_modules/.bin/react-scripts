#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/Flowise/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.23.3_@babel+core@7.24.0__@babel+plugin-transf_qxntvakccrohkdxqa7kgj4oebi/node_modules/react-scripts/bin/node_modules:/home/user/Flowise/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.23.3_@babel+core@7.24.0__@babel+plugin-transf_qxntvakccrohkdxqa7kgj4oebi/node_modules/react-scripts/node_modules:/home/user/Flowise/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.23.3_@babel+core@7.24.0__@babel+plugin-transf_qxntvakccrohkdxqa7kgj4oebi/node_modules:/home/user/Flowise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/Flowise/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.23.3_@babel+core@7.24.0__@babel+plugin-transf_qxntvakccrohkdxqa7kgj4oebi/node_modules/react-scripts/bin/node_modules:/home/user/Flowise/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.23.3_@babel+core@7.24.0__@babel+plugin-transf_qxntvakccrohkdxqa7kgj4oebi/node_modules/react-scripts/node_modules:/home/user/Flowise/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.23.3_@babel+core@7.24.0__@babel+plugin-transf_qxntvakccrohkdxqa7kgj4oebi/node_modules:/home/user/Flowise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-scripts/bin/react-scripts.js" "$@"
else
  exec node  "$basedir/../react-scripts/bin/react-scripts.js" "$@"
fi
