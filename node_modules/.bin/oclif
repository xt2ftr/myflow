#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/Flowise/node_modules/.pnpm/oclif@3.17.2_@swc+core@1.4.6_@types+node@20.12.12_encoding@0.1.13_mem-fs@2.3.0_typescript@5.5.2/node_modules/oclif/bin/node_modules:/home/user/Flowise/node_modules/.pnpm/oclif@3.17.2_@swc+core@1.4.6_@types+node@20.12.12_encoding@0.1.13_mem-fs@2.3.0_typescript@5.5.2/node_modules/oclif/node_modules:/home/user/Flowise/node_modules/.pnpm/oclif@3.17.2_@swc+core@1.4.6_@types+node@20.12.12_encoding@0.1.13_mem-fs@2.3.0_typescript@5.5.2/node_modules:/home/user/Flowise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/Flowise/node_modules/.pnpm/oclif@3.17.2_@swc+core@1.4.6_@types+node@20.12.12_encoding@0.1.13_mem-fs@2.3.0_typescript@5.5.2/node_modules/oclif/bin/node_modules:/home/user/Flowise/node_modules/.pnpm/oclif@3.17.2_@swc+core@1.4.6_@types+node@20.12.12_encoding@0.1.13_mem-fs@2.3.0_typescript@5.5.2/node_modules/oclif/node_modules:/home/user/Flowise/node_modules/.pnpm/oclif@3.17.2_@swc+core@1.4.6_@types+node@20.12.12_encoding@0.1.13_mem-fs@2.3.0_typescript@5.5.2/node_modules:/home/user/Flowise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../oclif/bin/run" "$@"
else
  exec node  "$basedir/../oclif/bin/run" "$@"
fi
