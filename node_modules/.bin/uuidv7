#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/Flowise/node_modules/.pnpm/uuidv7@0.6.3/node_modules/uuidv7/node_modules:/home/user/Flowise/node_modules/.pnpm/uuidv7@0.6.3/node_modules:/home/user/Flowise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/Flowise/node_modules/.pnpm/uuidv7@0.6.3/node_modules/uuidv7/node_modules:/home/user/Flowise/node_modules/.pnpm/uuidv7@0.6.3/node_modules:/home/user/Flowise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uuidv7/cli.js" "$@"
else
  exec node  "$basedir/../uuidv7/cli.js" "$@"
fi
