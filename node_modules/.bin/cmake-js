#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/Flowise/node_modules/.pnpm/cmake-js@7.3.0/node_modules/cmake-js/bin/node_modules:/home/user/Flowise/node_modules/.pnpm/cmake-js@7.3.0/node_modules/cmake-js/node_modules:/home/user/Flowise/node_modules/.pnpm/cmake-js@7.3.0/node_modules:/home/user/Flowise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/Flowise/node_modules/.pnpm/cmake-js@7.3.0/node_modules/cmake-js/bin/node_modules:/home/user/Flowise/node_modules/.pnpm/cmake-js@7.3.0/node_modules/cmake-js/node_modules:/home/user/Flowise/node_modules/.pnpm/cmake-js@7.3.0/node_modules:/home/user/Flowise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cmake-js/bin/cmake-js" "$@"
else
  exec node  "$basedir/../cmake-js/bin/cmake-js" "$@"
fi
