#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/Flowise/node_modules/.pnpm/jsondiffpatch@0.6.0/node_modules/jsondiffpatch/bin/node_modules:/home/user/Flowise/node_modules/.pnpm/jsondiffpatch@0.6.0/node_modules/jsondiffpatch/node_modules:/home/user/Flowise/node_modules/.pnpm/jsondiffpatch@0.6.0/node_modules:/home/user/Flowise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/Flowise/node_modules/.pnpm/jsondiffpatch@0.6.0/node_modules/jsondiffpatch/bin/node_modules:/home/user/Flowise/node_modules/.pnpm/jsondiffpatch@0.6.0/node_modules/jsondiffpatch/node_modules:/home/user/Flowise/node_modules/.pnpm/jsondiffpatch@0.6.0/node_modules:/home/user/Flowise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jsondiffpatch/bin/jsondiffpatch.js" "$@"
else
  exec node  "$basedir/../jsondiffpatch/bin/jsondiffpatch.js" "$@"
fi
